<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_server_info_form" model="ir.ui.view">
        <field name="name">Server info form</field>
        <field name="model">server.info</field>
        <field name="arch" type="xml">
            <form string="Warehouse Server Form">
                <group>
                    <group>
                        <field name="name"/>
                        <field name="server_gen"/>
                    </group>

                </group>
            </form>
        </field>
    </record>
    <record id="action_server_info" model="ir.actions.act_window">
        <field name="name">Server info action</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">server.info</field>
        <field name="view_mode">tree,form,kanban</field>
    </record>


</odoo>
